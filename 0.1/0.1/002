#include<iostream>
#include<string>
using namespace std;
int main()
{
    string s;
    cin>>s;
    int a=s.size();
    cout<<a; 
    int sz[a];
    int sum=0;
    for(int i=0;i<a;i++)
    {sz[i]=s[i]-'0';
    sum+=sz[i];
    	}
    string b=to_string(sum);
    string arr[10]={"zero","one","two","three","four","five","six","seven","eight","nine"};
  cout<<arr[b[0]-'0'];
    for(int i=1;i<b.length();i++)
        cout<<" "<<arr[b[i]-'0'];
    return 0;
    
    
}
